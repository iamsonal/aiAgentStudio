<?xml version="1.0" encoding="UTF-8"?>
<CustomMetadata xmlns="http://soap.sforce.com/2006/04/metadata" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <label>Vehicle Identification Number (VIN)</label>
    <protected>false</protected>
    <values>
        <field>Category__c</field>
        <value xsi:type="xsd:string">Identity</value>
    </values>
    <values>
        <field>Description__c</field>
        <value xsi:type="xsd:string">Detects Vehicle Identification Numbers (VIN) when preceded by VIN, vehicle identification, or chassis keywords. Matches exactly 17 characters using the ISO 3779 character set (letters A-Z excluding I, O, Q; digits 0-9).</value>
    </values>
    <values>
        <field>IsActive__c</field>
        <value xsi:type="xsd:boolean">true</value>
    </values>
    <values>
        <field>MaskFormat__c</field>
        <value xsi:type="xsd:string">[VIN:{index}]</value>
    </values>
    <values>
        <field>PatternRegex__c</field>
        <value xsi:type="xsd:string">(?i)\b(?:vin|vehicle\s*(?:identification\s*)?(?:number|id)|chassis(?:\s*number)?)\b[#:\s]*([A-HJ-NPR-Z0-9]{17})\b</value>
    </values>
    <values>
        <field>Priority__c</field>
        <value xsi:type="xsd:double">37.0</value>
    </values>
    <values>
        <field>RequiresValidation__c</field>
        <value xsi:type="xsd:boolean">false</value>
    </values>
</CustomMetadata>
