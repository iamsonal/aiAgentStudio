<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>LinkType__c</fullName>
    <description>Defines the relationship path from the starting point to the target data (for &apos;Record Anchored&apos; sources).</description>
    <inlineHelpText>&apos;Direct&apos;: The starting record is the target. &apos;Parent Lookup&apos;: Follow a lookup field up. &apos;Child Relationship&apos;: Get related child records down. Defaults to &apos;Direct&apos; if blank.</inlineHelpText>
    <label>Link Type</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Picklist</type>
    <valueSet>
        <restricted>true</restricted>
        <valueSetDefinition>
            <sorted>false</sorted>
            <value>
                <fullName>Direct</fullName>
                <default>true</default>
                <label>Direct</label>
            </value>
            <value>
                <fullName>Parent Lookup</fullName>
                <default>false</default>
                <label>Parent Lookup</label>
            </value>
            <value>
                <fullName>Child Relationship</fullName>
                <default>false</default>
                <label>Child Relationship</label>
            </value>
        </valueSetDefinition>
    </valueSet>
</CustomField>
